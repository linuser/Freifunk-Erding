+/*Freifunk Logo (background)*/
+
+.ff-logo-bg {
+  background-attachment: scroll;
+  background-image: url(bootstrap/images/freifunk-logo.svg);
+  background-position: 94% 17%;
+  background-repeat: no-repeat;
+  background-size: auto 25% auto;
+}
+
+.ff-logo-bg .container {
+  padding-right: 6em;
+}
+
+/* CLOUDS and other fancy backgrounds! */
+
+.cloudy-bg {
+	background-attachment: fixed;
+	background-image: url(bootstrap/images/wolke.min.png);
+	background-position: 50% 5%;
+	background-repeat: no-repeat;
+	background-size: 200% auto;
+}
+
+/* take care of elements that have cloud+logo */
+
+.cloudy-bg.ff-logo-bg {
+  background-attachment: fixed, scroll;
+  background-image: url(bootstrap/images/wolke.min.png), url(bootstrap/images/freifunk-logo.svg);
+  background-position: 50% 5%, 94% 17%;
+  background-size: 200% auto, 25% auto;
+}
+
+/* larger-than-mobile overides for cloud and cloud+logo bg*/
+
+@media (min-width: 768px) {
+  {
+    /* cut container on right side for less type on logo */
+  
+  }
+  .cloudy-bg {
+    background-position: 50% 5%;
+    background-size: 125% auto;
+  }
+  .cloudy-bg.ff-logo-bg {
+    background-position: 50% 50%, 96% 63.5%;
+    background-size: 125% auto, 25% auto;
+  }
+  .ff-logo-bg .container {
+    padding-right: 6em;
+  }
+}
+
+/* the medium size makes the cloud look good on tablets… */
+
+@media (min-width: 768px) and (max-width: 1200px) {
+  .cloudy-bg {
+    background-position: 50% 5%;
+  }
+  .cloudy-bg.ff-logo-bg {
+    background-position: 50% 5%, 96% 63.5%;
+  }
+}
