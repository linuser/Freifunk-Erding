+/* ALERTS*/
+
+.alert {
+  border-width: 3px;
+  -webkit-box-shadow: none;
+          box-shadow: none;
+}
+
+.alert > .alert-link,
+.alert p > .alert-link,
+.alert small > .alert-link,
+.alert ul > .alert-link,
+.alert ol > .alert-link,
+.alert dl > .alert-link,
+.alert > :link,
+.alert p > :link,
+.alert small > :link,
+.alert ul > :link,
+.alert ol > :link,
+.alert dl > :link {
+  font-weight: bold;
+  color: inherit;
+  text-decoration: underline;
+}
+
+.alert.close,
+.alert > .close {
+  opacity: 1;
+  filter: alpha(opacity=100);
+}
+
+.alert.close:hover,
+.alert > .close:hover,
+.alert.close:focus,
+.alert > .close:focus {
+  opacity: 1;
+  filter: alpha(opacity=100);
+}
